 Programmer:Bhaskar Reddy Krishnamarteddypalli
 Purpose: To convert a given decimal number to Binary
 */
 
#include<stdio.h>
int main()
{
	int n,i=0;
	int a[12];
	printf("Enter the number for conversion\n");
	scanf("%d",&n);
	while(n>0)
	{
		a[i]=n%2;
		i++;
		n=n/2;	
	}
	for(i=i-1;i>=0;i--)
	{
		printf("%d",a[i]);
	}	
		return 0;
}
